{
  "name": "电商助手",
  "version": "0.1",
  "manifest_version": 3,
  "description": "电商助手",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["sidePanel"],
  "side_panel": {
    "default_path": "sidepanel/index.html"
  },
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "default_title": "电商助手"
  }
}
